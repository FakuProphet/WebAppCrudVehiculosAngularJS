

<!DOCTYPE html>

<html ng-app="miApp">
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <title>Registrar Vehiculo</title>
</head>
<body ng-controller="HomeController">
    
    <div class="container">
        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <div class="form-group">
                    <label>Nombre</label>
                    <input type="text" ng-model="Vehiculo.nombre" />
                </div>
                <div class="form-group">
                    <label>Marca</label>
                    <input type="text" ng-model="Vehiculo.marca" />
                </div>
                <div class="form-group">
                    <label>Color</label>
                    <input type="text" ng-model="Vehiculo.color" />
                </div>
                <div class="form-group">
                    <label>Anio</label>
                    <input type="text" ng-model="Vehiculo.anio" />
                </div>
                <div class="form-group">
                    <label>Dominio</label>
                    <input type="text" ng-model="Vehiculo.dominio" />
                </div>
                <div class="form-group">
                    <input type="button" class="btn btn-primary" ng-click="saveData()" value="{{btn}}" />
                </div>
            </div>
            <div class="col-sm-2"></div>
        </div>
    </div>
    
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/AngularController/Home.js"></script>
</body>
</html>

